    <shape
        android:shape="oval"
        android:innerRadius="20dp">
        <corners android:radius="10dp"/>

        <gradient android:angle="90"

    android:startColor="#ffbb00"
    android:endColor="#ff2b2e"/>

    </shape>

</item>

</selector>